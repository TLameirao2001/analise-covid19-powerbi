	measure 'Rácio Reprodução' =
			
			VAR Pais =
			    LASTNONBLANKVALUE(
			        'Dados Covid'[date],
			         MAX('Dados Covid'[reproduction_rate])
			    )
			VAR Mundo = CALCULATE(
			                LASTNONBLANKVALUE(
			                    'Dados Covid'[date],
			                    MAX('Dados Covid'[reproduction_rate])
			                ),
			            'Dados Covid'[location] = "World"
			            )
			RETURN
			IF(
			    HASONEVALUE('Dados Covid'[location]),
			    Pais,
			    Mundo
			)
